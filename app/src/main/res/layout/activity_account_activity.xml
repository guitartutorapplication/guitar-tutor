<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_account_activity"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jlo19.guitartutor.activities.AccountActivityActivity">

    <include android:id="@+id/toolbar"
        layout="@layout/toolbar"/>

    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/toolbar"
        android:id="@+id/listView"
        android:layout_margin="@dimen/big_layout_margin"
        android:divider="@android:color/transparent"
        android:dividerHeight="@dimen/small_layout_margin" />

</RelativeLayout>
